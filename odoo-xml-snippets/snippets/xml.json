  "Action Server": {
    "prefix": "<ir_action_server>",
    "body": [
      "<record id=\"${1:action_model_name_action_name}\" model=\"ir.actions.server\">",
      "\t<field name=\"name\">Action ${2:Name}</field>",
      "\t<field name=\"model_id\" ref=\"${3:modul.model_name}\"/>",
      "\t<field name=\"state\">code</field>",
      "\t<field name=\"code\"><![CDATA[",
      "for order in ${4:records}:",
      "\tproperty_type = env['${5:x_modul.property.type}'].sudo().search([('${6:x_name}', '=', '${7:Other}')], limit=1)",
      "\tenv['${8:x_modul.property}'].sudo().create({",
      "\t\t'${9:x_name}': order.name,",
      "\t\t'${10:x_expected_price}': 0,",
      "\t\t'${11:x_selling_price}': 0,",
      "\t\t'${12:x_sale_order_id}': order.id,",
      "\t\t'${13:x_property_type_id}': property_type.id,",
      "\t})",
      "]]></field>",
      "</record>",
      "$0"
    ],
    "description": "Server action executing Python code"
  },
