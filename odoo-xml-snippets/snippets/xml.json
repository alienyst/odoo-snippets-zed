  "Field Computed": {
    "prefix": "<ir_field_computed>",
    "body": [
      "<record id=\"${1:field_model_name}\" model=\"ir.model.fields\">",
      "\t<field name=\"model_id\" ref=\"${2:modul.model_name}\"/>",
      "\t<field name=\"name\">x_${3:name}</field>",
      "\t<field name=\"field_description\">${4:Total Area}</field>",
      "\t<field name=\"ttype\">${5|float,integer,monetary|}</field>",
      "\t<field name=\"depends\">${6:field_depends}</field>",
      "\t<field name=\"compute\"><![CDATA[",
      "for record in self:",
      "\trecord['x_${3:name}'] = record.${6:field_depends} * 2",
      "]]></field>",
      "</record>",
      "$0"
    ],
    "description": "Computed field with depends and code"
  },
