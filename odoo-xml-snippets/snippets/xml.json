      "\t<field name=\"model_id\" ref=\"${3:modul.model_name}\"/>",
      "\t<field name=\"trigger\">${4|on_create,on_write,on_create_or_write,on_time,on_state_set|}</field>",
      "\t<field name=\"trg_selection_field_id\" ref=\"${5:modul.model_selection_field_name_id}\"/>",
      "\t<field name=\"trigger_field_ids\" eval=\"[(4, ref('${6:modul.model_field_selection_id}'))]\"/>",
      "\t<field name=\"action_server_ids\" eval=\"[(4, ref('${7:modul.action_model_name_action_name}'))]\"/>",
      "</record>",
      "$0"
    ],
    "description": "Base automation rule triggering a server action"
  },
  "Action Server Controller Example": {
    "prefix": "<ir_action_server_controller>",
    "body": [
      "<record id=\"server_action${1:_controller_example}\" model=\"ir.actions.server\">",
      "\t<field name=\"name\">Example ${2:Controller}</field>",
      "\t<field name=\"model_id\" ref=\"${3:modul.model_name}\"/>",
      "\t<field name=\"website_published\">${4|True,False|}</field>",
      "\t<field name=\"website_path\">${5:estate}</field>",
      "\t<field name=\"state\">code</field>",
      "\t<field name=\"code\"><![CDATA[",
      "html = '<html><body><h1>${6:Properties}</h1><ul>'",
      "for record in request.env['${7:x_modul.property}'].sudo().search([]):",
      "\thtml += f\"<li>{record.${8:x_name}}</li>\"",
      "html += '</ul></body></html>'",
      "response = request.make_response(html)",
      "]]></field>",
      "</record>",
      "$0"
    ],
    "description": "Server action exposed as website controller"
  },
